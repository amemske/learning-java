package com.example;
import com.example.*;

public class HelloWorld {
    public static void main(String[] args) {
        System.out.println("Hello, World!");
        
        int x = 10;

        if (x > 5) {

            System.out.print("Hello");

            if (x < 15) {

                System.out.println("World");

            }

        }
        
    }
  

}
